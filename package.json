{
  "name": "dotenv-manipulator",
  "version": "2.1.1",
  "description": "dotenv-manipulator allows you to add, remove or update both environment variables and .env files on runtime",
  "main": "index.js",
  "scripts": {
    "test": "rm -rf ./.env && touch ./.env && mocha test/mocha.js",
    "docs": "node_modules/.bin/jsdoc index.js -t node_modules/minami -d ./docs"
  },
  "author": "JiPai",
  "license": "GPL-3.0",
  "dependencies": {
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0"
  },
  "repository": "github:JiPaix/dotenv-manipulator",
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc": "^3.6.4",
    "minami": "^1.2.3",
    "mocha": "^8.0.1"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=5.6.0"
  },
  "keywords": [
    "dotenv",
    "environment",
    ".env",
    "runtime",
    "process.env"
  ]
}

{
  "name": "dotenv-manipulator",
  "version": "1.3.1",
  "description": "dotenv-manipulator allows you to add, remove or update both environment variables and .env files on runtime",
  "main": "index.js",
  "scripts": {
    "test": "rm -rf ./.env && touch ./.env && mocha test/mocha.js",
    "docs": "node_modules/.bin/jsdoc index.js -t node_modules/minami -d ./docs"
  },
  "author": "JiPai",
  "license": "GPL-3.0",
  "dependencies": {
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0"
  },
  "repository": "github:JiPaix/dotenv-manipulator",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsdoc": "^3.6.2",
    "minami": "^1.2.3",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.6"
  },
  "keywords": [
